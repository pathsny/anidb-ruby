{
  "presets" : [
    [
			"@babel/preset-env", {
				"targets": {
					"browsers": [
						"last 2 versions",
						"IE >= 10"
					]
				},
        "useBuiltIns": "entry"
			}
		],
    "@babel/preset-react"
  ],
  "plugins": [
    "react-hot-loader/babel",
    "@babel/plugin-proposal-class-properties",
    "@babel/plugin-proposal-optional-chaining",
  ]
}
